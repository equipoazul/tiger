let
 
    var N := 3
    function printint (i : int) =
      let 
      function f (i : int) =
          if i > 0 then 
             (f (i/10); 
              print (chr (i-i/10*10+ord ("0"))))
      in 
         if i < 0 then 
            (print ("-"); f (-i))
         else if 
            i>0 then f (i)
         else 
            print ("0")
      end
     
    function try (c : int) =
        for r := 0 to N do 
            printint(r)

 in 
   try (1); 0
 end

